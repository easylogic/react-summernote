  "description": "React extension for summernote",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "module": "dist/",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev:lite": "webpack-dev-server --config ./config/lite/webpack.dev.config.js --progress",
    "dev:bs4": "webpack-dev-server --config ./config/bs4/webpack.dev.config.js --progress",
    "dev": "npm run dev:bs4",
    "build:lite": "webpack --config ./config/lite/webpack.prod.config.js --progress",
    "build:bs4": "webpack --config ./config/bs4/webpack.prod.config.js --progress",
    "build": "npm run build:lite && npm run build:bs4",
