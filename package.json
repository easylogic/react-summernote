  "bugs": {
    "url": "https://github.com/easylogic/react-summernote/issues"
  },
  "homepage": "https://github.com/easylogic/react-summernote#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@types/jquery": "^3.3.33",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
